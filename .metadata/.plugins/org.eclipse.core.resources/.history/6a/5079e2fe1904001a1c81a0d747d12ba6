package FF;

import XH.For;

public class arraydemo {

	public static void main(String[] args) {
		double[]douarr=new double[] {2.0,4.0,1.59,13.5,1.7};
		for(int i=0;i<douarr.length;i++) {
		    System.out.print(" "+douarr);
	}
	 System.out.println();
	 //增强性循环
	 for(double x:douarr){
		 System.out.print(" "+x);
	 }

	}

}
