package szz;

public class YWD {
    public  static  void main(String[]args) {
    	//数字型变量
    	byte b1=4;
    	short a2=257;
        int i3=65547;
        long i4=456L;
        float f5=3.458997f  ;
        double d6=1.123456789123456789123456789;
        //字符型
        
        char c7='v';
        char c8='中';
        //布尔型	
        		
    System.out.println("严伟迪,用我一生换你十年天真无邪");
    }
}
