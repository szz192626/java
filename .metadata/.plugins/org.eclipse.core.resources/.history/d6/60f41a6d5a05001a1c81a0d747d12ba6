package FF;

public class NN {

	public static void main(String[] args) {
		package homework;

		import java.util.Scanner;

		import exercise.fordemo;
		import exercise.ifelse;

		public class hefashuzi {

			public static void main(String[] args) {
				// TODO Auto-generated method stub

				int num[] = new int[10];
				int count1 = 0;
				int count2 = 0;
				int count3 = 0;
				Scanner scan = new Scanner(System.in);
				System.out.println("请输入十个数：");
				for (int i = 0; i < num.length; i++) {
					System.out.println("请输入第" + (i + 1) + "个数字");
					num[i] = scan.nextInt();

					if (num[i] == 1) {
						count1 += 1;
						break;
					}
					if (num[i] == 2) {
						count2 += 1;
						break;
					}
					if (num[i] == 3) {
						count3 += 1;
						break;
					}

					System.out.println("数字1的个数为" + count1);
					System.out.println("数字2的个数为" + count2);
					System.out.println("数字3的个数为" + count3);
					System.out.println("不合法数字的个数为" + (10 - count1 - count2 - count3));

				}

			}
		}

		
		
		
		
		
		
		
		
		
		
		
		
		
		

	}

}
