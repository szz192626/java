package XH;

public class Forbreakhousework {

	public static void main(String[] args) {
		boolean flag=true;
		for(int i=0,f=0;f<=20;i++,f+=i) {
			System.out.println(f);
			if(f>20) {
				System.out.println("跳出，程序结束。");
				flag=false;
				break;
			}
			if(flag==true) {
				System.out.println("当前累加值"+f);
			}
			
		}
		
		
		
		
		
		

	}

}
