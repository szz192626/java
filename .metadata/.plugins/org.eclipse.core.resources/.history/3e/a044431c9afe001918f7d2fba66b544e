package XH;

import java.util.Scanner;

public class For {

	public static void main(String[] args) {
		Scanner scanner=new Scanner(System.in);
        System.out.println("请输入五位同学的成绩：");
		for(int i=0;i<5;i++) {
			System.out.println("请输入第"+(i+1)+"门课的成绩");
			System.out.println  (scanner.nextInt());
			
		}
		
	}

}
